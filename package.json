{
  "name": "express-babel-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-server": "babel server --out-dir dist",
    "build": "npm run clean && npm run build-server",
    "clean": "rm -rf dist && mkdir dist",
    "test": "NODE_ENV=test nyc mocha ./tests/index.spec.js --timeout 5000 --exit",
    "start:dev": "nodemon ./server/app.js --exec babel-node",
    "start": "node dist/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankly034/express-babel-setup.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frankly034/express-babel-setup/issues"
  },
  "homepage": "https://github.com/frankly034/express-babel-setup#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.15.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "sqlite3": "^4.1.0"
  },
  "nyc": {
    "exclude": [
      "tests"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
